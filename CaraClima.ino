void setup() {
  Serial.begin(9600);

  dht.begin();

  WiFi.begin(ssid, password);

  /* while ( WiFi.status() != WL_CONNECTED ) {
    delay ( 500 );
    Serial.print ( "." );
  } */

  timeClient.begin();
  timeClient.setTimeOffset(-(3600*7));
  
  delay(250);
  display.begin(i2c_Address, true);

  display.display();
  delay(2000);
  display.clearDisplay();
  
  
}


void loop() {
  
  timeClient.update();

  h = dht.readHumidity();
  t = dht.readTemperature();

  showBitmap();

  display.setTextSize(2);
  display.setTextColor(SH110X_WHITE);
  
  display.setCursor(45, 4); 
  display.print(t, 1);  
  display.drawCircle(96, 4, 3, SH110X_WHITE);
  display.setCursor(102, 4);
  display.print("C");

  display.setCursor(45, 24);
  display.print(h, 1);
  display.println("%");

  display.setTextSize(2);
  display.setCursor(65, 44);
  display.print(timeClient.getFormattedTime().substring(0, 5));

  display.display();
  delay(5000);
  display.clearDisplay();

  if (t < 24) {
  showColdFace();

  display.setTextSize(1);
  display.setTextColor(SH110X_WHITE);
  
  display.setCursor(64, 4);
  display.print("Sensacion");
  display.setCursor(64, 14);
  display.print("termica: ");
  display.setCursor(64, 24);
  display.print("Hace frio");
  display.setCursor(64, 50);
  display.print("MOAM LAB");

  display.display();
  delay(2000);
  display.clearDisplay();
  } 
  
  if (t > 28) {
  showHotFace();

  display.setTextSize(1);
  display.setTextColor(SH110X_WHITE);
  
  display.setCursor(64, 4);
  display.print("Sensacion");
  display.setCursor(64, 14);
  display.print("termica: ");
  display.setCursor(64, 24);
  display.print("Hace calor");
  display.setCursor(64, 50);
  display.print("MOAM LAB");

  display.display();
  delay(2000);
  display.clearDisplay();
  } 

  if (t >= 24 && t <= 28) {
  showSmileFace();

  display.setTextSize(1);
  display.setTextColor(SH110X_WHITE);
  
  display.setCursor(64, 4);
  display.print("Sensacion");
  display.setCursor(64, 14);
  display.print("termica: ");
  display.setCursor(64, 24);
  display.print("Agradable");
  display.setCursor(64, 50);
  display.print("MOAM LAB");

  display.display();
  delay(2000);
  display.clearDisplay();
  }

Serial.println(timeClient.getFormattedTime().substring(0, 5));

}

